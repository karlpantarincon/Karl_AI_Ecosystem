# Configuración optimizada de base de datos
# Karl AI Ecosystem - Producción

# Pool de conexiones
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=30
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=3600

# Configuración de consultas
DB_ECHO=false
DB_ECHO_POOL=false

# Configuración de cache
DB_CACHE_SIZE=1000
DB_CACHE_TTL=300

# Configuración de índices
DB_INDEX_OPTIMIZATION=true
DB_QUERY_OPTIMIZATION=true
